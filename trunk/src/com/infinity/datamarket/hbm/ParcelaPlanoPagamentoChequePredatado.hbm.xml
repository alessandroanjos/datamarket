<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">   


<hibernate-mapping>
    <class name="com.infinity.datamarket.comum.pagamento.ParcelaPlanoPagamentoChequePredatado" table="PARCELA_PLAN_PAGTO_CHEQUE_PRE" dynamic-update="false" dynamic-insert="false" lazy="false">
        
        <composite-id name="pk" class="com.infinity.datamarket.comum.pagamento.ParcelaPlanoPagamentoChequePredatadoPK">			
	   		<key-many-to-one name="plano" class="com.infinity.datamarket.comum.pagamento.PlanoPagamentoChequePredatado" column="ID_PLANO"/>
            <key-property name="numeroEntrada" column="NUM_ENTRADA" type="integer"/>
         </composite-id>
	
		<property name="percentagemParcela" type="java.math.BigDecimal" update="true" insert="true">
              <column name="PERCENTAGEM_PARCELA" length="20" not-null="false"/>
        </property>
        
        <property name="quantidadeDias" type="int" update="true" insert="true">
              <column name="QTD_DIAS" length="20" not-null="false"/>
        </property>
	
    </class>

</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.infinity.datamarket.comum.util.Controle" table="CONTROLE" dynamic-update="false" dynamic-insert="false" lazy="false">

        <id
            name="chave"
            column="CHAVE"
            type="java.lang.String"
            length="50"
            unsaved-value="null"
        >
            <generator class="assigned">
            </generator>
        </id>


        <property name="valor" type="java.lang.Long" update="true" insert="true">
            <column name="VALOR" length="255" not-null="true"/>
        </property>

    </class>

</hibernate-mapping>

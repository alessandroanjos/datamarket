<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">   


<hibernate-mapping>
    <class name="com.infinity.datamarket.comum.estoque.AjusteEstoque" table="AJUSTE_ESTOQUE" dynamic-update="false" dynamic-insert="false" lazy="false">
        
        <id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="2"
            unsaved-value="null"
        >
            <generator class="assigned">
            </generator>
        </id>

	
        <property name="quantidadeAntes" type="java.math.BigDecimal" update="true" insert="true">
            <column name="QTD_ANTES" length="20" not-null="false"/>
        </property>
        
        <property name="codigoUsuario" type="java.lang.String" update="true" insert="true">
	        <column name="COD_USUARIO" length="100" not-null="true"/>
	    </property>
        
        <property name="quantidadeDepois" type="java.math.BigDecimal" update="true" insert="true">
            <column name="QTD_DEPOIS" length="20" not-null="false"/>
        </property>
        
        <property name="data" type="java.util.Date" update="false" insert="true">
            <column name="DATA" length="10" not-null="true"/>
        </property> 
        
        <many-to-one name="produto" class="com.infinity.datamarket.comum.produto.Produto" update="true" insert="true">
          <column name="ID_PRODUTO" not-null="false"/>
        </many-to-one>
        
        <many-to-one name="estoque" class="com.infinity.datamarket.comum.estoque.Estoque" update="true" insert="true">
          <column name="ID_ESTOQUE" not-null="false"/>
          <column name="ID_LOJA" not-null="false"/>
    	</many-to-one>
	
    </class>

</hibernate-mapping>

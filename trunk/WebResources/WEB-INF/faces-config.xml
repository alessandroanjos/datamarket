<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN" "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>
	
	<navigation-rule>
		<from-view-id>/jsp/login.jsp</from-view-id>
		<navigation-case>
			<from-action>#{loginBB.logar}</from-action>
			<from-outcome>logado</from-outcome>
			<to-view-id>/jsp/menu.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{loginBB.logar}</from-action>
			<from-outcome>erro</from-outcome>
			<to-view-id>/jsp/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/jsp/menu.jsp</from-view-id>
		<navigation-case>
			<from-outcome>inserirTipoProduto</from-outcome>
			<to-view-id>/jsp/tipoProduto/inserirTipoProduto.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>manterTipoProduto</from-outcome>
			<to-view-id>/jsp/tipoProduto/consultarTipoProduto.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>inserirUnidade</from-outcome>
			<to-view-id>/jsp/unidade/inserirUnidade.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>manterUnidade</from-outcome>
			<to-view-id>/jsp/unidade/consultarUnidade.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>inserirImposto</from-outcome>
			<to-view-id>/jsp/imposto/inserirImposto.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>manterImposto</from-outcome>
			<to-view-id>/jsp/imposto/consultarImposto.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>inserirLoja</from-outcome>
			<to-view-id>/jsp/loja/inserirLoja.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>manterLoja</from-outcome>
			<to-view-id>/jsp/loja/consultarLoja.jsp</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-action>#{loginBB.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/jsp/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/jsp/erro.jsp</from-view-id>
	</navigation-rule>
	
	

<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.infinity.datamarket.pdv.maquinaestados.Tecla" table="TECLA" dynamic-update="false" dynamic-insert="false" lazy="false">
        
        <id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="2"
            unsaved-value="null"
        >
            <generator class="assigned">
            </generator>
        </id>


        <property name="codigoASCI" type="int" update="true" insert="true">
            <column name="CODIGO_ASCI" length="20" not-null="true"/>
        </property>

    </class>

</hibernate-mapping>

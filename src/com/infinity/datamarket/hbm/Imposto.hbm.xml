<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"   
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.infinity.datamarket.comum.produto.Imposto" table="IMPOSTO" dynamic-update="false" dynamic-insert="false" lazy="false">
        
        <id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="2"
            unsaved-value="null"
        >
            <generator class="assigned">
            </generator>
        </id>


        <property name="descricao" type="java.lang.String" update="true" insert="true">
            <column name="DESCRICAO" length="50" not-null="true"/>
        </property>
        
	<property name="impostoImpressora" type="java.lang.String" update="true" insert="true">
            <column name="IMP_IMPRESSORA" length="50" not-null="true"/>
        </property>

	<property name="percentual" type="java.math.BigDecimal" update="true" insert="true">
            <column name="PERCENTUAL" length="50" not-null="true"/>
        </property>
    </class>

</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.infinity.datamarket.comum.pagamento.Autorizadora" table="AUTORIZADORA" dynamic-update="false" dynamic-insert="false" lazy="false">
        
       <id
            name="id"
            column="ID"
           
        >
            <generator class="increment">
            </generator>
        </id>



        <property name="descricao" type="java.lang.String" update="true" insert="true">
            <column name="DESCRICAO" length="50" not-null="true"/>
        </property>
        
        <property name="situacao" type="java.lang.String" update="true" insert="true">
            <column name="SITUACAO" length="1" not-null="true"/>
        </property>
        
        <property name="desagil" type="java.math.BigDecimal" update="true" insert="true">
            <column name="DESAGIL" length="15" not-null="true"/>
        </property>

    </class>

</hibernate-mapping>

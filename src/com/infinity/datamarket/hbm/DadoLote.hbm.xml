<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"   
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.infinity.datamarket.comum.lote.DadoLote" table="DADOS_LOTE" dynamic-update="false" dynamic-insert="false" lazy="false">
        
        <id
            name="lote"
            column="LOTE"
            type="int"
            length="8"
            unsaved-value="null"
        >
            <generator class="assigned">
            </generator>
        </id>


        <property name="dado" type="serializable" update="true" insert="true">
            <column name="DADO" not-null="false"/>
        </property>
        
        <property name="operacao" type="java.lang.String" update="true" insert="true">
            <column name="OPERACAO" length="1" not-null="false"/>
        </property>
        
        <property name="sequencial" type="int" update="false" insert="false">
            <column name="ID" length="8" not-null="false"/>
        </property>
    </class>

</hibernate-mapping>

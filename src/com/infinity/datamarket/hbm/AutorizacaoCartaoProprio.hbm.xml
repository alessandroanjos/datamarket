<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.infinity.datamarket.autorizador.AutorizacaoCartaoProprio" table="AUTORIZACAO_CARTAO_PROPRIO" dynamic-update="false" dynamic-insert="false" lazy="false">
        
        <id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="4"
        >
            <generator class="increment">
            </generator>
        </id>


        <property name="status" type="java.lang.String" update="true" insert="true">
            <column name="STATUS" length="50" not-null="true"/>
        </property>
        
        <property name="data" type="java.util.Date" update="true" insert="true">
            <column name="DATA" length="50" not-null="true"/>
        </property>
        
        <property name="valor" type="java.math.BigDecimal" update="true" insert="true">
            <column name="VALOR" length="15" not-null="true"/>
        </property>
        
        <many-to-one name="cliente" class="com.infinity.datamarket.comum.cliente.Cliente" update="true" insert="true">
          <column name="ID_CLIENTE" not-null="false"/>
        </many-to-one>

    </class>

</hibernate-mapping>

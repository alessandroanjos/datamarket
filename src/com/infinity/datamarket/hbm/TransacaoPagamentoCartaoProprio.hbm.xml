<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<joined-subclass
		name="com.infinity.datamarket.comum.transacao.TransacaoPagamentoCartaoProprio"
		extends="com.infinity.datamarket.comum.transacao.TransacaoPagamento"
		table="TRANSACAO_PAGTO_CARTAO_PROPRIO" lazy="false">

		<key>
			<column name="LOJA" />
			<column name="COMPONENTE" />
			<column name="NUM_TRANSACAO" />
			<column name="DATA_TRANSACAO" />
		</key>
		
		<property name="CPFCNPJ" type="java.lang.String"
			update="true" insert="true">
			<column name="CPFCNPJ" length="20"
				not-null="false" />
		</property>
		
	</joined-subclass>
</hibernate-mapping>

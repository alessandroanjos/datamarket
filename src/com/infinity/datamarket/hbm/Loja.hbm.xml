<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.infinity.datamarket.comum.usuario.Loja" table="LOJA" dynamic-update="false" dynamic-insert="false" lazy="false">
        <id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="2"
            unsaved-value="null"
        >
            <generator class="assigned">
            </generator>
        </id>

        <property name="nome" type="java.lang.String" update="true" insert="true">
            <column name="NOME" length="50" not-null="true"/>
        </property>

		<property name="numeroIp" type="java.lang.String" update="true" insert="true">
            <column name="NUMERO_IP" length="15" not-null="true"/>
        </property>
        
        <property name="numeroPorta" type="java.lang.String" update="true" insert="true">
            <column name="NUMERO_PORTA" length="4" not-null="true"/>
        </property>
        
         <property name="idEstoque" type="java.lang.Long" update="true" insert="true">
            <column name="ID_ESTOQUE_ATUAL" length="4" not-null="false"/>
        </property>
        
        <set name="usuarios" table="USUARIO_LOJA">
             <key column="ID_LOJA"/>
             <many-to-many column="ID_USUARIO" class="com.infinity.datamarket.comum.usuario.Usuario"/>
        </set>
    </class>

</hibernate-mapping>

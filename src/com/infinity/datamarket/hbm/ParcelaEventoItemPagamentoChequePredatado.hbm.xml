<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

   <hibernate-mapping>
      <class name="com.infinity.datamarket.comum.transacao.ParcelaEventoItemPagamentoChequePredatado" table="PARC_EVE_ITEM_PAG_CHEQUE_PRE" lazy="false">

         <composite-id name="pk" class="com.infinity.datamarket.comum.transacao.ParcelaEventoItemPagamentoChequePredatadoPK">
           <key-property name="loja" column="LOJA" type="integer"/>
           <key-property name="componente" column="COMPONENTE" type="integer"/>
           <key-property name="numeroTransacao" column="NUM_TRANSACAO" type="integer"/>
           <key-property name="dataTransacao" column="DATA_TRANSACAO" type="java.util.Date"/>
           <key-property name="numeroEvento" column="NUM_EVENTO" type="integer"/>
           <key-property name="numeroParcela" column="NUM_PARCELA" type="integer"/>
         </composite-id>

          <property name="CPFCNPJ" type="java.lang.String" update="true" insert="true">
            <column name="CPF_CNPJ" length="50" not-null="true"/>
          </property>
          
          <property name="numeroChequeLido" type="java.lang.String" update="true" insert="true">
            <column name="NUMERO_CHEQUE_LIDO" length="50" not-null="false"/>
          </property>
          
          <property name="banco" type="java.lang.String" update="true" insert="true">
            <column name="BANCO" length="50" not-null="false"/>
          </property>
          
          <property name="agencia" type="java.lang.String" update="true" insert="true">
            <column name="AGENCIA" length="50" not-null="false"/>
          </property>
          
          <property name="conta" type="java.lang.String" update="false" insert="true">
            <column name="CONTA" length="50" not-null="false"/>
          </property>
          
          <property name="numeroCheque" type="java.lang.String" update="true" insert="true">
            <column name="NUMERO_CHEQUE" length="50" not-null="false"/>
          </property>
          
          <property name="entrada" type="java.lang.String" update="true" insert="true">
            <column name="ENTRADA" length="1" not-null="false"/>
          </property>
          
          <property name="valor" type="java.math.BigDecimal" update="true" insert="true">
              <column name="VALOR" length="20" not-null="false"/>
          </property>
          
          <property name="data" type="java.util.Date" update="true" insert="true">
            <column name="DATA" length="20" not-null="false"/>
          </property>
          
      </class>
   </hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE hibernate-mapping   
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.infinity.datamarket.infocomponent.InfoComponent" table="INFO_COPONENTE" dynamic-update="false" dynamic-insert="false" lazy="false">

        <id
            name="componente"
            column="COMPONENTE"
            type="java.lang.String"
            length="50"
            unsaved-value="null"
        >
            <generator class="assigned">
            </generator>
        </id>


        <property name="versao" type="java.lang.String" update="true" insert="true">
            <column name="VERSAO" length="50" not-null="true"/>
        </property>

	<property name="lote" type="java.lang.String" update="true" insert="true">
            <column name="LOTE" length="50" not-null="true"/>
        </property>

	<property name="estado" type="java.lang.String" update="true" insert="true">
            <column name="ESTADO" length="50" not-null="true"/>
        </property>

	<property name="dataAtualizacao" type="java.util.Date" update="true" insert="true">
            <column name="ATUALIZACAO" length="50" not-null="true"/>
        </property>

    </class>

</hibernate-mapping>
